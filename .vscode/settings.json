{
    "cSpell.words": [
        "healthz",
        "stretchr",
        "vgeshiktor",
        "webserver"
    ],
    "editor.fontSize": 16,
    "editor.codeActionsOnSave": {},
    "go.formatTool": "gofmt",
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast",
        "--allow-parallel-runners"
    ],
    "go.toolsEnvVars": {
        "emeraldwalk.runonsave": {
            "commands": [
                {
                    "match": ".*\\.go$",
                    "cmd": "workbench.action.tasks.runTask golangci-lint"
                }
            ]
        }
    }
}